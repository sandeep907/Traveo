@model Shipping.ViewModels.Roles.RolesSearchViewModel

<section class="content">
    <!-- Default box -->
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">Roles</h3>

            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                        title="Collapse">
                    <i class="fa fa-minus"></i>
                </button>
                <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                    <i class="fa fa-times"></i>
                </button>
            </div>
        </div>
        <section>
            @if (ViewBag.Message != null)
            {
                @Html.Raw(HttpUtility.HtmlDecode(ViewBag.Message))
            }
            <div class="row">
                <div class="col-xs-12">
                    <div class="box">
                        <div class="box-header">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="Form-control">
                                        <input type="text" class="form-control" placeholder="Search" id="rolesserch">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="dvroleslist">
                            @Html.Partial("_RolesList", Model)
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <div id="NewRole"></div>
        <div class="box-footer">
            surycourier  private limited
        </div>
        <!-- /.box-footer-->
    </div>
    <!-- /.box -->
</section>